{
  "name": "example-browser-ts-react-i18n",
  "version": "1.0.0",
  "main": "dist-release/index.js",
  "typings": "dist-release/src/index.d.ts",
  "webpack": "src/index.tsx",
  "private": true,
  "scripts": {
    "prebuild": "ws-build-translations",
    "build": "webpack-cli --config src/webpack.config.js",
    "prestorybook": "ws-build-translations",
    "storybook:build": "build-storybook",
    "storybook:watch": "start-storybook -p 9001",
    "lint": "ws lint",
    "preunit": "ws-build-translations",
    "unit": "jest --config tests/jest.config.js"
  },
  "ws": {
    "type": "browser",
    "i18n": {
      "locales": [
        "en_GB",
        "en_US",
        "de_DE",
        "de_AT"
      ]
    }
  },
  "dependencies": {
    "@mercateo/ws-intl": "^1.0.3",
    "@types/react": "^16.0.0",
    "intl-messageformat": "^1.3.0",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@mercateo/ws": "link:../ws",
    "@storybook/addon-a11y": "^4.0.0-alpha.18",
    "@storybook/addon-actions": "^4.0.0-alpha.18",
    "@storybook/addon-backgrounds": "^4.0.0-alpha.18",
    "@storybook/addon-knobs": "^4.0.0-alpha.18",
    "@storybook/addon-storyshots": "^4.0.0-alpha.18",
    "@storybook/addon-storyshots-puppeteer": "^4.0.0-alpha.18",
    "@storybook/addon-storysource": "^4.0.0-alpha.18",
    "@storybook/addon-viewport": "^4.0.0-alpha.18",
    "@storybook/cli": "^4.0.0-alpha.18",
    "@storybook/react": "^4.0.0-alpha.18",
    "@types/enzyme": "^3.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.0",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "intl": "^1.2.4",
    "jest": "^23.5.0",
    "webpack-cli": "^3.1.0"
  }
}
