{
  "name": "example-spa-ts-i18n",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "ws-build-translations",
    "build": "webpack-cli --config src/webpack.config.js",
    "lint": "ws lint",
    "preunit": "ws-build-translations",
    "unit": "jest --config tests/jest.config.js",
    "watch": "webpack-serve --config src/webpack.config.js"
  },
  "ws": {
    "type": "spa",
    "i18n": {
      "locales": [
        "en_GB",
        "en_US",
        "de_DE",
        "de_AT"
      ],
      "features": [
        "app",
        "common"
      ],
      "importUrl": "https://raw.githubusercontent.com/donaldpipowitch/ws-translations-demo/master/{feature}/{locale}.properties"
    }
  },
  "dependencies": {
    "@mercateo/ws-intl": "^1.0.3",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "babel-polyfill": "^6.26.0",
    "example-browser-ts-react": "*",
    "example-browser-ts-react-i18n": "*",
    "intl": "^1.2.4",
    "intl-messageformat": "^1.3.0",
    "normalize.css": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@mercateo/ws": "link:../ws",
    "@types/enzyme": "^3.0.0",
    "@types/jest": "^23.3.0",
    "@types/webpack-env": "^1.13.6",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.2",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
