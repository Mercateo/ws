    "normalize.css": "^7.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@mercateo/ws": "link:../ws",
    "@types/enzyme": "^3.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.0",
    "@types/webpack-env": "^1.13.6",
    "enzyme": "^3.0.0",
