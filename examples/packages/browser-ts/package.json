{
  "name": "example-browser-ts",
  "version": "1.0.0",
  "main": "dist-release/index.js",
  "webpack": "src/index.ts",
  "private": true,
  "scripts": {
    "ws": "node ../../../bin/ws",
    "unit": "jest --config tests/jest.config.js"
  },
  "ws": {
    "type": "browser",
    "targets": {
      "browsers": "last 2 Chrome versions, last 1 Safari versions, last 2 Firefox versions, Explorer 9-11"
    },
    "selenium": {
      "host": "ondemand.saucelabs.com",
      "port": 80,
      "envUser": "SAUCE_USERNAME",
      "envPassword": "SAUCE_ACCESS_KEY",
      "filterForAvailability": true
    }
  },
  "typings": "dist-release/src/index.d.ts",
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "jest": "^23.1.0"
  }
}
